<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- CONFIGURAÇÕES DE LOG -->
		<add key="logFileName" value="log.txt" />
		<add key="logFilePath" value="logs" />
		<add key="logSuccessEnabled" value="true" />
		<add key="logErrorEnabled" value="true" />
		<add key="logInfoEnabled" value="true" />

		<!-- CREDENCIAIS DE ACESSO AO BANCO -->
		<add key="dbHost" value="localhost" />
		<add key="dbPort" value="5432" />
		<add key="dbName" value="Company" />
		<add key="dbUser" value="postgres" />
		<add key="dbPass" value="1234" />
	</appSettings>
</configuration>